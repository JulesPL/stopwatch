{
    "name": "stopwatch_mvc",
    "version": "0.1.0",
    "description": "iOS 9 Stopwatch + VanillaJS MVC Architecture + Webpack 3",
    "main": "index.js",
    "scripts": {
        "build": "webpack",
        "build:prod": "webpack -p",
        "start": "npm run build:prod && http-server ./dist/ -a $(ipconfig getifaddr en0) -o",
        "start:local": "npm run build:prod && http-server ./dist/ -a localhost -o",
        "dev": "webpack-dev-server --host $(ipconfig getifaddr en0)",
        "dev:local": "webpack-dev-server",
        "watch": "webpack --watch"
    },
    "keywords": [],
    "author": "Mikis Woodwinter",
    "license": "MIT",
    "devDependencies": {
        "babel-core": "~6.26.0",
        "babel-loader": "~7.1.2",
        "babel-preset-env": "~1.6.0",
        "clean-webpack-plugin": "~0.1.17",
        "css-loader": "~0.28.7",
        "extract-text-webpack-plugin": "~3.0.0",
        "file-loader": "~1.1.4",
        "html-loader": "~0.5.1",
        "html-webpack-plugin": "~2.30.1",
        "http-server": "^0.11.1",
        "node-sass": "^4.9.3",
        "sass-loader": "~6.0.6",
        "style-loader": "~0.18.2",
        "url-loader": "^0.6.2",
        "webpack": "~3.6.0",
        "webpack-dev-server": "^3.1.14"
    },
    "dependencies": {
        "correcting-interval": "^2.0.0"
    }
}
